<div data_id="business<%= business.id %>">
	<h3>
		<a href="#/businesses/<%= business.id %>">
			<%= business.escape("name") %>
		</a> Rates: <%= business.escape("stars") %>
	</h3>
	<p><%= business.escape("street_address") %></p>
	<p><%= business.escape("city") %>, <%= business.escape("state") %> <%= business.escape("zip") %></p>
</div>