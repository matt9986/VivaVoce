<div id='mapSpace' class='mapSpace'></div>
<ul>
	<% businesses.each(function (business) { %>
		<li>
		<%= JST['businesses/_business_partial']({business: business}) %>
		</li>
	<% }) %>
  <li><% if(businesses.page > 1){ %>
      <a data-id='-1' 
         class="page" 
         href='#prior'
        >Previous</a>  <% }; %>
    Page: <%= businesses.page %>
    <% if(businesses.length === 10){ %>
      <a
        class="page" 
        data-id='1'
        href='#next'>More</a><% } %></li>
	<li>Can't find what you're looking for? <a href="#/businesses/new">Add it</a></li>
</ul>
